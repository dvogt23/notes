{ 
    "gitbook":     "3.2.3",
    "root":        "./Notes",
    "title":       "Knowledge book",
    "description": "Fixable notes of my life and experience",
    "author":      "Dmitrij Vogt <divogt@dima23.de> (https://dima23.de)",
    "structure": { 
        "readme": "README.md",
        "summary": "SUMMARY.md" 
    },
    "plugins": [
        "toolbar",
        "-highlight", 
        "anchorjs"
    ],
    "pluginsConfig": {
        "sharing": {
            "facebook":   false,
            "twitter":    false,
            "google":     false,
            "weibo":      false,
            "instapaper": false,
            "vk":         false,
            "all": ["twitter", "facebook", "google"]
        },
        "toolbar": {
            "buttons": [
                {
                    "label": "GitHub",
                    "icon": "fa fa-github",
                    "url": "https://github.com/dvogt23"
                },
                {
                    "label": "Web",
                    "icon": "fa fa-globe",
                    "url": "https://dima23.de"
                }
            ]
        }
    }
}
